    <script>
        //查体中问题3、4当选择无时不可选择其他选项
       var docheckBox = function (dom) {
          var name = $(dom)[0].name;
          var selectValue = $("input[name="+name+"]:checked").customVal();
          if ($.inArray("无",selectValue) > -1  && selectValue.length > 1) {
              $("input[name="+name+"]:checked").customVal(["无"]);
          }
        };
        //疾病史当选择无时不可选择其他选项
        var doSelect = function (dom) {
            var vali = $(dom).customVal();
            if ($.inArray("无",vali) > -1 && vali.length > 1) {
                $(dom).customVal(["无"]);
            }
        };
        //饮酒、吸烟当选择从不时其他无关问题禁用
        var doLimit = function (dom) {
            var name = dom.name;
            var patt = new RegExp("从不");
            var na = name.split("1")[0];
            if (name ==="drink_history-item_1") {
                var v_len =5;
            }else if (name ==="smoke_history-item_1") {
                var v_len =4;
            }else {
                var v_len = 4;
            }
            if (patt.test($("input[name='"+name+"']:checked").val())) {
                for (var i = 2; i < v_len;i++) {
                    var j_len = $("input[name='"+na+i+"']").length;
                    for (var j = 0; j < j_len;j++){
                        $("input[name='"+na+i+"']")[j].disabled = true;
                        $("input[name='"+na+i+"']")[j].checked = false;
                    }
                }
            }else{
                for (var i = 2; i < v_len;i++) {
                    var j_len = $("input[name='"+na+i+"']").length;
                    for (var j = 0; j < j_len;j++){
                     $("input[name='"+na+i+"']")[j].disabled = false;
                    }
                }
            }
        }
        $(function() {
            $('input[type=radio][data-bindtext]').each(function() {
                $(this).click(function() {
                    radioOrText(this);
                });
            });
            var uid = ~~getvl('id');
            if (uid) {
                doAjax(window.baseUrl + "/patient_record/check", 'get', {'id': uid}, getUserInfoCallback);
            }
        });
        var radioOrText = function(dom) {
            if (jQuery(dom).data("hastext")) {
                jQuery("#" + jQuery(dom).data("bindtext")).removeAttr("disabled").focus();
            } else {
                jQuery("#" + jQuery(dom).data("bindtext")).attr("disabled","disabled").val("");
            }
        };

        var cbFun = function(data, status) {
            if (!status) {
                swal("提交失败", JSON.parse(data.reponseText)["error"], "error");
            } else {
                swal("提交成功", "正在进入患者列表", "success");
                setTimeout(function() {
                    location.href="/patientList";
                },1000);
                return true;
            }
        };
        var submitForm = function(formDom) {
            var obj = $('#recordForm').getValue(),
                postObj = {
                    "health_status": {},
                    "family_history": {},
                    "drink_history": {},
                    "smoke_history": {},
                    "eating_habit": {},
                    "exercise_habit": {},
                    "sleep_habit": {},
                    "emotional_state": {},
                    "environment_state": {},
                    "professional_state": {},
                    "excretion": {},
                    "check_body": {},
                    "physical_examination": {}
                };
            for (var i in obj) {
                var isChild = i.split("-");
                if (isChild.length > 1) {
                    postObj[isChild[0]][isChild[1]] = obj[i];
                } else {
                    postObj[isChild[0]] = obj[i];
                }
            }
            doAjax(
                window.baseUrl + "/patient_record/create",
                "post",
                JSON.stringify(postObj),
                cbFun,
                {},
                {contentType: 'application/json;charset=utf-8'}
            );
            return 0;
        };
        var showBirthSex = function(idCardDom) {
            var val = idCardDom.value = idCardDom.value.toLocaleUpperCase();
            if (checkIdCard(val)) {
                var bas = val.match(/^\d{6}(\d{4})(\d{2})(\d{2})\d{2}(\d)(\d|X)$/);
                $("#born").val(bas[1] + "-" + bas[2] + "-" + bas[3]);
                $("#sex" + (~~bas[4]%2)).prop("checked",true);
            } else {
                $("#born").val("");
                $("input:radio[id^='sex']").prop("checked",false);
            }
        };
        var getUserInfoCallback = function(data, status) {
            if (data && status) {
                for (var i in data) {
                    if (typeof data[i]==="string") {
                        $("*[name='" + i + "']").customVal(data[i]);
                    } else if (typeof data[i]==="object") {
                        for (var k in data[i]) {
                            var domId = i + "-" + k;
                            $("*[name='" + domId + "']").customVal(data[i][k]);
                        }
                    }
                }
            };
        };
    </script>

