<%- include header %>
<div class="page">
  <div class="page-content container">
      <form class="form-horizontal" role="form" method="post" action="" onsubmit="submitForm(this); return false;">
          <div class="row">
            <div class="col-md-12">
              <!-- Example Panel With Heading -->
              <div class="panel panel-bordered" >
                <div class="panel-heading">
                  <h3 class="panel-title">体检信息录入</h3>
                </div>

                <div class="panel-body">
                  <div class="row">
					<div class="col-xs-12 col-md-4 offset-md-1">
						<h3 class="text-middle">个人信息录入</h3>
						<div class="form-group form-material floating" data-plugin="formMaterial">
						  <input type="text" class="form-control empty" name="name">
						  <label class="floating-label">姓名</label>
						</div>
						   <div class="form-group form-material">
							<label class="form-control-label" for="sex">性别</label>
							<div style="margin-top: 12px;">
								<div class="radio-custom radio-primary radio-inline">
								  <input type="radio" id="sex1" name="sex">
								  <label for="sex1">男</label>
								</div>
								<div class="radio-custom radio-primary radio-inline">
								  <input type="radio" id="sex2" name="sex">
								  <label for="sex2">女</label>
								</div>
							</div>
						  </div>
						<div class="form-group form-material floating" data-plugin="formMaterial">
						  <input type="text" class="form-control empty" name="id_number">
						  <label class="floating-label">身份证号</label>
						</div>
						<div class="form-group form-material floating" data-plugin="formMaterial">
						  <input type="text" class="form-control empty" name="nation">
						  <label class="floating-label">手机号码</label>
						</div>
					</div>

					<div class="col-xs-12 col-md-4 offset-md-1">
						<h3 class="text-middle">采集信息录入</h3>
						<div class="row">
							<div class="col-xs-12 col-md-6">
								<div class="form-group form-material">
									<label class="form-control-label" for="min_pressure">舒张压</label>
									<div class="input-group">
										<input type="text" class="form-control" id="min_pressure" name="min_BloodPressure">
										<span class="input-group-addon">毫米汞柱</span>
									</div>
								</div>
							</div>
							<div class="col-xs-12 col-md-6">
								<div class="form-group form-material">
									<label class="form-control-label" for="max_pressure">收缩压</label>
									<div class="input-group">
										<input type="text" class="form-control" id="max_pressure" name="max_BloodPressure">
										<span class="input-group-addon">毫米汞柱</span>
									</div>
								</div>
							</div>

							<div class="col-xs-12 col-md-6">
								<div class="form-group form-material">
									<label class="form-control-label" for="b_sugar">餐前血糖</label>
									<div class="input-group">
										<input type="text" class="form-control" id="b_sugar" name="b_BloodGlucose">
										<span class="input-group-addon">毫克/分升</span>
									</div>
								</div>
							</div>
							<div class="col-xs-12 col-md-6">
								<div class="form-group form-material">
									<label class="form-control-label" for="a_sugar">餐后血糖</label>
									<div class="input-group">
										<input type="text" class="form-control" id="a_sugar" name="a_BloodGlucose">
										<span class="input-group-addon">毫克/分升</span>
									</div>
								</div>
							</div>

							<div class="col-xs-12 col-md-12">
								<div class="form-group form-material">
									<label class="form-control-label" for="id_number_c">体重</label>
									<div class="input-group">
										<input type="text" class="form-control" id="id_number_c" name="weight">
										<span class="input-group-addon">cm</span>
									</div>
								</div>
								<div class="form-group form-material">
									<label class="form-control-label" for="id_number_c1">心率</label>
									<div class="input-group">
										<input type="text" class="form-control" id="id_number_c1" name="heart_rate">
										<span class="input-group-addon">次/分</span>
									</div>
								</div>
							</div>
						</div>
					</div>
					<div class="col-xs-12 col-md-9  offset-md-1">
						<button type="submit" class="btn btn-block btn-success">提交</button>
					</div>
				   </div>
                </div>
              </div>
              <!-- End Example Panel With Heading -->
            </div>
          </div>
      </form>
    </div>
  </div>


<%- include footer %>
